#!/bin/bash

#SBATCH -o ./out/mil/out_dinov2_vitb_bloom.txt
#SBATCH -e ./out/mil/error_dinov2_vitb_bloom.txt

#SBATCH -J vitb_bloom
#SBATCH -p gpu_p
#SBATCH -c 28
#SBATCH --mem=240G
#SBATCH -q gpu_long
#SBATCH --time=96:00:00
#SBATCH --nice=10000
#SBATCH --gres=gpu:1
##SBATCH -C a100_40gb
# Environment setup

source /home/aih/${USER}/.bashrc
conda activate dinov2

export LD_LIBRARY_PATH=/usr/lib/wsl/lib:$LD_LIBRARY_PATH

python ../wbc_mil.py --data_path dinov2_vitb14_bloom --wandb_name wbc_mil_dinov2_vitb14_bloom