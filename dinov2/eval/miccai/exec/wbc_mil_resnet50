#!/bin/bash

#SBATCH -o ./out/out_resnet50.txt
#SBATCH -e ./out/error_resnet50.txt

#SBATCH -J resnet50
#SBATCH -p gpu_p
#SBATCH -c 28
#SBATCH --mem=240G
#SBATCH -q gpu_long
#SBATCH --time=96:00:00
#SBATCH --nice=10000
#SBATCH --gres=gpu:1
#SBATCH -C a100_40gb
# Environment setup

source /home/aih/${USER}/.bashrc
conda activate dinov2

export LD_LIBRARY_PATH=/usr/lib/wsl/lib:$LD_LIBRARY_PATH

python ../wbc_mil.py --data_path resnet50 --wandb_name wbc_mil_resnet50